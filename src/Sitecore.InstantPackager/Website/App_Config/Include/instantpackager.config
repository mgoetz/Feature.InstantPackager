<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:x="http://www.sitecore.net/xmlconfig/">
	<sitecore>
		<commands>
			<command name="velir:getthisip" type="Sitecore.SharedSource.InstantPackager.CustomSitecore.Commands.DownloadItemPackage, Sitecore.SharedSource.InstantPackager" />
			<command name="velir:getip" type="Sitecore.SharedSource.InstantPackager.CustomSitecore.Commands.GetPackage, Sitecore.SharedSource.InstantPackager" />
			<command name="velir:addtoip" type="Sitecore.SharedSource.InstantPackager.CustomSitecore.Commands.AddItem, Sitecore.SharedSource.InstantPackager" />
			<command name="velir:removefromip" type="Sitecore.SharedSource.InstantPackager.CustomSitecore.Commands.RemoveItem, Sitecore.SharedSource.InstantPackager" />
			<command name="velir:addsubtoip" type="Sitecore.SharedSource.InstantPackager.CustomSitecore.Commands.AddSubItems, Sitecore.SharedSource.InstantPackager" />
			<command name="velir:removesubfromip" type="Sitecore.SharedSource.InstantPackager.CustomSitecore.Commands.RemoveSubItems, Sitecore.SharedSource.InstantPackager" />
			<command name="velir:addrelatedtoip" type="Sitecore.SharedSource.InstantPackager.CustomSitecore.Commands.AddRelatedItems, Sitecore.SharedSource.InstantPackager" />
			<command name="velir:removerelatedfromip" type="Sitecore.SharedSource.InstantPackager.CustomSitecore.Commands.RemoveRelatedItems, Sitecore.SharedSource.InstantPackager" />
			<command name="velir:clearip" type="Sitecore.SharedSource.InstantPackager.CustomSitecore.Commands.Clear, Sitecore.SharedSource.InstantPackager" />
			<command name="velir:viewip" type="Sitecore.SharedSource.InstantPackager.CustomSitecore.Commands.View, Sitecore.SharedSource.InstantPackager" />
			<!-- <command name="velir:saveip" type="Sitecore.SharedSource.InstantPackager.CustomSitecore.Commands.Save, Sitecore.SharedSource.InstantPackager" /> -->
			<!-- <command name="velir:loadip" type="Sitecore.SharedSource.InstantPackager.CustomSitecore.Commands.Load, Sitecore.SharedSource.InstantPackager" /> -->
		</commands>
		<controlSources>
			<!-- This allows the instant package management list to be picked up by Sitecore as registered Xaml applications -->
			<source mode="on" namespace="Sitecore.Web.UI.XmlControls" folder="/bin" deep="false"/>
		</controlSources>
	</sitecore>
</configuration>
